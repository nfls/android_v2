<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="io.nfls.williamxie.nflser">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/nflsio"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/nflsio"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="io.nfls.williamxie.nflser.LoginActivity"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="io.nfls.williamxie.nflser.HomeActivity">
        </activity>
        <activity android:name="io.nfls.williamxie.nflser.SignUpActivity">
        </activity>
        <activity android:name="io.nfls.williamxie.nflser.ResetPasswordActivity">
        </activity>
        <activity android:name="io.nfls.williamxie.nflser.ResourcesActivity">
        </activity>
        <activity android:name="io.nfls.williamxie.nflser.PdfViewerActivity">
        </activity>
        <activity android:name="io.nfls.williamxie.nflser.ImageViewerActivity">
        </activity>
    </application>

</manifest>